{
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/masakik/template"
        }
    ],
    "require": {
        "gabordemooij/redbean": "^5.3",
        "raelgc/template": "dev-master",
        "mikecao/flight": "^1.3",
        "oomphinc/composer-installers-extender": "^1.1",
        "bower-asset/bootstrap": "^4.3",
        "bower-asset/angular": "^1.7",
        "monolog/monolog": "^2.0@dev",
        "twig/twig": "2.x-dev"
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "www/assets/{$vendor}/{$name}/": [
                "type:bower-asset",
                "type:npm-asset"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "USPdev\\View\\": "view/",
            "USPdev\\app\\": "app/"
        }
    },
    "minimum-stability": "dev"
}

{
    "require": {
        "gabordemooij/redbean": "^5.3",
        "raelgc/template": "^2.2",
        "mikecao/flight": "^1.3",
        "oomphinc/composer-installers-extender": "^1.1",
        "bower-asset/bootstrap": "^4.3",
        "bower-asset/angular": "^1.7"
    },
    "extra": {
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "www/assets/{$vendor}/{$name}/": ["type:bower-asset", "type:npm-asset"]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "dev"
}
